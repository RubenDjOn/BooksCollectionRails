
<%= form_for(@book) do |f| %>

<div class="field">
	<%= f.label :Title %><br>
	<%= f.text_field :title %>
</div>
<div class="field">
	<%= f.label :Description %><br>
	<%= f.text_field :description %>
</div>
<div class="field">
	<%= f.label :Pages %><br>
	<%= f.text_field :pages %>
</div>
<div class="actions">
	<%= f.submit %>
</div>

<% end %>


